{
  "name": "@toy-box/form-formula",
  "version": "1.0.2",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish --access public",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "antlr4ts": "antlr4ts ./src/formula/FormulaLexer.g4 ./src/formula/FormulaParser.g4"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@formily/core": "^2.0.0-beta.50",
    "@formily/json-schema": "^2.0.0-beta.48",
    "@formily/react": "^2.0.0-beta.49",
    "@formulajs/formulajs": "^2.6.9",
    "antd": "^4.15.5",
    "antlr4ts": "^0.5.0-alpha.4",
    "classnames": "^2.3.1",
    "codemirror": "^5.61.0",
    "react": "^16.12.0",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.0",
    "@umijs/test": "^3.0.5",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "dumi": "^1.0.14",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "yorkie": "^2.0.0"
  }
}
